<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/item_groupList_0_relativ"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_80sdp">


    <ImageView
        android:layout_marginStart="@dimen/_10sdp"
        android:layout_centerInParent="true"
        android:layout_alignParentStart="true"
        android:id="@+id/item_groupList_2_imageView"
        android:src="@drawable/logo_xml"
        android:layout_width="@dimen/_70sdp"
        android:layout_height="@dimen/_70sdp"/>

    <LinearLayout
        android:layout_marginTop="@dimen/_10sdp"
        android:gravity="top"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_toEndOf="@id/item_groupList_2_imageView"
        android:layout_centerInParent="true"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="@dimen/_16ssp"
            android:id="@+id/item_groupList_3_title"
            android:text="@string/navSearch_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <TextView
            android:id="@+id/item_groupList_4_desc"
            android:text="@string/navSearch_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>




</RelativeLayout>